# MongoDB Configuration
# Copy this file to .env and modify as needed

# Authentication
MONGO_INITDB_ROOT_USERNAME=admin
MONGO_INITDB_ROOT_PASSWORD=admin123
MONGO_INITDB_DATABASE=banesco_test

# Connection Settings
MONGO_HOST=mongodb
MONGO_PORT=27017

# Database Names
MONGO_DATABASE_ANALYTICS=banesco_analytics
MONGO_DATABASE_WAREHOUSE=banesco_warehouse

# Security Settings
MONGO_AUTH_ENABLED=true
MONGO_SSL_ENABLED=false
